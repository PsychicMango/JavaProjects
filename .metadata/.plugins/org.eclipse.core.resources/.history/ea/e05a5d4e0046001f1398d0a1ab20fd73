package Programa;

import java.util.ArrayList;
import java.util.List;

public class Lista {

	public static void main(String[] args) {
		List<Integer> list = new ArrayList<>();
		
		list.add(1);
		list.add(10);
		list.add(100);
		list.add(1000);
		list.add(10000);
		for(int i : list) {
			System.out.println(i);
		}

	}

}
